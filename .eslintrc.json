{
  "env": {
    "commonjs": true,
    "node": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "varsIgnorePattern": "res",
        "args": "after-used",
        "ignoreRestSiblings": false
      }
    ]
  }
}
